# AETHER-GEN Frontend Configuration
# Rename this to .env.local for local development
# Or set as environment variables

# Backend API URL
VITE_API_URL=http://localhost:8000

# Application Configuration
VITE_APP_NAME=AETHER-GEN
VITE_APP_VERSION=1.5.0

# Feature Flags
VITE_ENABLE_CHAT=true
VITE_ENABLE_BOM=true
VITE_ENABLE_EXPORT=true
VITE_ENABLE_POINT_E=true

# UI Configuration
VITE_THEME=dark
VITE_ANIMATIONS=true

# Performance
VITE_RENDER_QUALITY=high
VITE_POINT_CLOUD_SIZE=512
